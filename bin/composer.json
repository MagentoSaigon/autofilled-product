{
  "require": {
    "zookal/harris-street-impex": "dev-master",
    "magento-ecg/coding-standard": "^2.0",
    "inchoo/php7": "dev-master",
    "mercuriel/autofilled": "*@dev",
    "aoepeople/aoe_scheduler": "^1.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.firegento.com"
    },
    {
      "type": "path",
      "url": "../src/modules/*/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "vcs",
      "url": "https://github.com/monsieurbiz/Mbiz_Setup"
    }
  ],
  "extra":{
    "magento-root-dir": "../src/magento/",
    "auto-append-gitignore": true,
    "magento-deploystrategy": "symlink",
    "magento-force":true
  },
  "config":{
    "secure-http":false,
    "bin-dir": "bin"
  },
  "autoload": { "psr-0": {
    "": [
      "../src/magento/app",
      "../src/magento/app/code/local",
      "../src/magento/app/code/community",
      "../src/magento/app/code/core",
      "../src/magento/lib"
    ]
  }},
  "require-dev": {
    "phpunit/phpunit": "^5.5"
  },
  "minimum-stability": "dev"
}
